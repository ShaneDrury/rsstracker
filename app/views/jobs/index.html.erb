<table>
  <% @jobs.each do |job| %>
  <tr>
    <td>
      <%= job.id %>
    </td>
    <td>
      <pre>
        <%= job.handler %>
      </pre>
    </td>
    <td>
      <%= job.last_error %>
    </td>
    <td>
      <%= job.created_at %>
    </td>
    <td>
      <%= job.run_at %>
    </td>
  </tr>
  <% end %>
</table>
